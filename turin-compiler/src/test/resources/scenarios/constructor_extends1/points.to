namespace points

type Point {
    int x
    int y
    int z default 0
    double distanceFromOrigin = Math.sqrt(x*x + y*y + z*z)
}

type LabelledPoint extends Point {
    String label
}

LabelledPoint foo1() = LabelledPoint(1, 2, "hi")
LabelledPoint foo2() = LabelledPoint(1, 2, "hi", 3)
LabelledPoint foo3() = LabelledPoint(y=2, x=1, "hi", 3)
LabelledPoint foo4() = LabelledPoint(y=2, z=3, x=1, label="hi")
LabelledPoint foo5() = LabelledPoint(y=2, x=1, label="hi")
